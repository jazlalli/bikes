{
  "name": "bikes",
  "version": "0.0.1",
  "description": "Bike comparison website",
  "main": "index.js",
  "scripts": {
    "seed": "./bin/init.sh",
    "build": "mkdir -p dist/ && browserify app/main.js -t [ babelify --presets [ es2015 react stage-0 ] ] > dist/bundle.js",
    "start": "npm run build && babel-node server.js"
  },
  "keywords": [
    "bikes",
    "comparison",
    "compare"
  ],
  "author": "Jaz Lalli",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "mongoose": "~4.4.10",
    "react": "~0.14.8",
    "react-dom": "~0.14.8",
    "react-router": "~2.0.1",
    "serve-static": "^1.10.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babelify": "~7.2.0",
    "browserify": "~13.0.0",
    "eslint": "~2.5.3",
    "eslint-config-pebblecode": "~1.5.0",
    "eslint-plugin-react": "~4.2.3",
    "nodemon": "~1.9.1",
    "uglify-js": "~2.6.2",
    "watchify": "~3.7.0"
  }
}
